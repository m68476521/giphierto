<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/home_graph"
    app:startDestination="@id/trending">

    <fragment
        android:id="@+id/trending"
        android:name="com.m68476521.giphierto.home.TrendingFragment"
        android:label="fragment_mainHome"
        tools:layout="@layout/fragment_trending">
        <action
            android:id="@+id/action_trending_to_giphDialog"
            app:destination="@id/giphDialog" />
    </fragment>

    <fragment
        android:id="@+id/giphDialog"
        android:name="com.m68476521.giphierto.GiphDialog"
        android:label="GiphDialog">
        <argument
            android:name="image"
            android:defaultValue='""'
            app:argType="string" />

        <argument
            android:name="id"
            android:defaultValue='""'
            app:argType="string" />

        <argument
            android:name="imageOriginal"
            android:defaultValue='""'
            app:argType="string" />

        <argument
            android:name="title"
            android:defaultValue='""'
            app:argType="string" />
    </fragment>
</navigation>